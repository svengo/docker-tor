# Paths
DataDirectory /data

# Ports
ORPort $ORPORT
DirPort $DIRPORT
SocksPort $TOR_SOCKS_PORT

# Bandwidth limits
RelayBandwidthRate $RELAY_BANDWIDTH_RATE
RelayBandwidthBurst $RELAY_BANDWIDTH_BURST

# ExitPolicy
ExitPolicy $EXITPOLICY

# Control port
ControlPort $CONTROLPORT
HashedControlPassword $HASHEDCONTROLPASSWORD

# Server infos
Nickname  $NICKNAME
ContactInfo $CONTACTINFO
MyFamily $MYFAMILY
Address $ADDRESS

# SOCKS policy
SocksPolicy accept *
